# https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 2 spaces indent for yaml files
[*.yaml]
indent_style = space
indent_size = 2
[*.yaml.dec]
indent_style = space
indent_size = 2
